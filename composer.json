{
    "name": "aura/framework",
    "type": "aura-package",
    "description": "The Aura Framework package, when combined with a system skeleton, provides a full-stack framework built from Aura component packages.",
    "keywords": [ "framework" ],
    "homepage": "http://auraphp.github.com/Aura.Framework",
    "version": "1.0.0-dev",
    "license": "BSD-2-Clause",
    "authors": [
        {
            "name": "Aura Contributors",
            "homepage": "https://github.com/auraphp/Aura.Framework/contributors"
        }
    ],
    "require": {
        "php": ">=5.4.0",
        "aura/installer-default": "dev-develop",
        "aura/autoload": "dev-develop",
        "aura/cli": "dev-develop",
        "aura/di": "dev-develop",
        "aura/filter": "dev-develop",
        "aura/http": "dev-develop",
        "aura/intl": "dev-develop",
        "aura/marshal": "dev-develop",
        "aura/router": "dev-develop",
        "aura/session": "dev-develop",
        "aura/signal": "dev-develop",
        "aura/sql": "dev-develop",
        "aura/uri": "dev-develop",
        "aura/view": "dev-develop",
        "aura/web": "dev-develop"
    },
    "autoload": {
        "psr-0": {
            "Aura\\Framework": "src/"
        }
    }
}
